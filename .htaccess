# ai-timer-api â€” rewrite random paths to scripts (cache-bust by path)
RewriteEngine On

# TIME: /ai-timer-api/time/<anything>
RewriteRule ^time/.*$ time.php [L,QSA]

# TIMER: /ai-timer-api/timer/<minutes>
RewriteRule ^timer/([0-9]{1,3})/?$ timer.php?m=$1 [L,QSA]

# Optional: deny directory listing
Options -Indexes
